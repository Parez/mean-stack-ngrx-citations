{
  "name": "citations",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "nodemon dist/server/server.js",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "ng build --prod && npm run _server:build",
    "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",
    "_server:build": "tsc -p ./server -w",
    "play:build": "tsc -p ./server/playground",
    "play": "npm run play:build && nodemon dist/server/playground/index.js",
    "fill-citations": "run mongoimport --db citations --collection citations < server/db/test-data/citations.json",
    "fill-users": "run mongoimport --db citations --collection users < server/db/test-data/users.json",
    "fill-db": "npm run fill-users && npm run fill-citations"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~2.1.1",
    "@angular/compiler": "~2.1.1",
    "@angular/core": "~2.1.1",
